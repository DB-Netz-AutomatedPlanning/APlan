{
    "type": "object",
    "properties": {
        "$type": {
            "type": "string"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z_0-9]*$"
        },
        "version": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToTrackEdgeSection": {
           "type": "array",
            "items": {
                "type": "string",
                "format": "uuid"
            },
            "minItems": 1
        },
        "azimuthAngle": {
            "type": "number",
            "minimum": 0,
            "maximum": 359.999,
            "multipleOf": 0.001
        }
    },
    "required": [
        "id",
        "name",
        "version",
        "appliesToTrackEdgeSection",
        "azimuthAngle"
    ],
    "additionalProperties": false
}