{
    "type": "object",
    "properties": {
        "$type": {
            "type": "string"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z_0-9]*$"
        },
        "version": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToTrackArea": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToAllocationSection": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToDpsGroup": {
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string",
                    "format": "uuid"
                }
            ]
        },
        "appliesToDpsRight": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uuid"
            },
            "minItems": 0,
            "maxItems": 2
        },
        "appliesToDpsLeft": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uuid"
            },
            "minItems": 0,
            "maxItems": 2
        },
        "reversalTime": {
            "type": [
                "number",
                "null"
            ],
            "minimum": 0,
            "maximum": 20,
            "multipleOf": 0.1
        }
    },
    "required": [
        "id",
        "name",
        "version",
        "appliesToTrackArea",
        "appliesToAllocationSection",
        "appliesToDpsGroup",
        "appliesToDpsRight",
        "appliesToDpsLeft",
        "reversalTime"
    ],
    "additionalProperties": false
}