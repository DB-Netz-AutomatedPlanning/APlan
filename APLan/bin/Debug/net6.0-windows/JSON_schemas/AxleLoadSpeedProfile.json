{
    "type": "object",
    "properties": {
        "$type": {
            "type": "string"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z_0-9]*$"
        },
        "version": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToTrackEdgeSection": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uuid"
            },
            "minItems": 1
        },
        "trainEndApplicability": {
            "type": [
                "string",
                "null"
            ],
            "enum": [
                "Train length delay on validity end point of profile element.",
                "No Train length delay on validity end point of profile element."
            ]
        },
        "speed": {
            "type": "integer",
            "minimum": 0,
            "maximum": 600
        },
        "axleLoadCategory": {
            "type": "string",
            "enum": [
                "A",
                "HS17",
                "B1",
                "B2",
                "C2",
                "C3",
                "C4",
                "D2",
                "D3",
                "D4",
                "D4XL",
                "E4",
                "E5"
            ]
        }
    },
    "required": [
        "id",
        "name",
        "version",
        "appliesToTrackEdgeSection",
        "trainEndApplicability",
        "speed",
        "axleLoadCategory"
    ],
    "additionalProperties": false
}