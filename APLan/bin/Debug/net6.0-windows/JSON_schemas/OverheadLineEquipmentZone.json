{
    "type": "object",
    "properties": {
        "$type": {
            "type": "string"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z_0-9]*$"
        },
        "version": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToTrackArea": {
            "type": "string",
            "format": "uuid"
        },
        "length": {
            "type": "number",
            "minimum": 0.001,
            "maximum": 999999.999,
            "multipleOf": 0.001
        },
        "width": {
            "type": "number",
            "minimum": 0.001,
            "maximum": 999999.999,
            "multipleOf": 0.001
        },
        "height": {
            "type": "number",
            "minimum": 0.001,
            "maximum": 999999.999,
            "multipleOf": 0.001
        },
        "verticalTrackAreaOffset": {
            "type": "number",
            "minimum": 0,
            "maximum": 99.999,
            "multipleOf": 0.001
        }
    },
    "required": [
        "id",
        "name",
        "version",
        "appliesToTrackArea",
        "length",
        "width",
        "height",
        "verticalTrackAreaOffset"
    ],
    "additionalProperties": false
}