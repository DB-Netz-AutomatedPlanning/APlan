{
    "type": "object",
    "properties": {
        "$type": {
            "type": "string"
        },
        "version": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToTrackNode": {
            "type": "array",
            "minItems": 1,
            "maxItems": 4
        },
        "appliesToTrackArea": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToAllocationSection": {
            "type": "string",
            "format": "uuid"
        },
        "reversalTime": {
            "type": [
                "number"
            ],
            "minimum": 0,
            "maximum": 20
        },
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9_-]*$"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        }
    },
    "additionalProperties": false,
    "required": [
        "version",
        "appliesToTrackNode",
        "appliesToTrackArea",
        "appliesToAllocationSection",
        "reversalTime",
        "name",
        "id"
