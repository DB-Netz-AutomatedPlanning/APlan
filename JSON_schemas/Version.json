                "engineered",
                "validated",
                "ready for test",
                "ready for operation",
                "preloaded",
                "activated"
            ]
        },
        "successor": {
            "type": [
                "array",
                "null"
            ],
            "items": {
                "type": "string",
                "format": "uuid"
            }
