{
    "type": "object",
    "properties": {
        "$type": {
            "type": "string"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z_0-9]*$"
        },
        "version": {
            "type": "string",
            "format": "uuid"
        },
        "gauge": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    null,
                    "750",
                    "1000",
                    "1435",
                    "1520",
                    "1524",
                    "1600",
                    "1668"
                ]
            }
        },
        "length": {
            "type": "number",
            "minimum": 0.001,
            "maximum": 999999.999,
            "multipleOf": 0.001
        },
        "hasStartTrackNode": {
            "type": "string",
            "format": "uuid"
        },
        "hasEndTrackNode": {
            "type": "string",
            "format": "uuid"
        }
    },
    "required": [
        "id",
        "name",
        "version",
        "gauge",
        "length",
        "hasStartTrackNode",
        "hasEndTrackNode"
    ],
    "additionalProperties": false
}