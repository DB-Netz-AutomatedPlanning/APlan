{
    "type": "object",
    "properties": {
        "$type": {
            "type": "string"
        },
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "name": {
            "type": "string",
            "pattern": "^[a-zA-Z_0-9]*$"
        },
        "version": {
            "type": "string",
            "format": "uuid"
        },
        "appliesToTrackEdgeSection": {
           "type": "array",
            "items": {
                "type": "string",
                "format": "uuid"
            },
            "minItems": 1
        },
        "radius": {
            "type": "number",
            "minimum": -99999.99,
            "maximum": 99999.99,
            "multipleOf": 0.01
        },
        "initialArcLength": {
            "type": "number",
            "minimum": 0,
            "maximum": 99999.99,
            "multipleOf": 0.01
        },
        "hasCenterAtGeoCoordinates": {
            "type": "string",
            "format": "uuid"
        }
    },
    "required": [
        "id",
        "name",
        "version",
        "appliesToTrackEdgeSection",
        "radius",
        "initialArcLength",
        "hasCenterAtGeoCoordinates"
    ],
    "additionalProperties": false
}