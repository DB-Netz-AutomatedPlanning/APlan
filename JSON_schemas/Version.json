            "minimum": 1,
            "maximum": 999999
        },
        "created": {
            "type": "string",
            "format": "date-time"
        },
        "modified": {
            "type": [
                "string",
                "null"
            ],
            "format": "date-time"
        },
        "deleted": {
            "type": [
                "string",
                "null"
            ],
            "format": "date-time"
        },
